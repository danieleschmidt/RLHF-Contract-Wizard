{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "name": "rlhf-contract-wizard-staging",
    "namespace": "rlhf-staging",
    "labels": {
      "app": "rlhf-contract-wizard",
      "environment": "staging",
      "version": "3.0.0"
    }
  },
  "spec": {
    "replicas": 2,
    "selector": {
      "matchLabels": {
        "app": "rlhf-contract-wizard",
        "environment": "staging"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "rlhf-contract-wizard",
          "environment": "staging",
          "version": "3.0.0"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "rlhf-contract-wizard",
            "image": "terragon/rlhf-contract-wizard:3.0.0-stag-1756347057",
            "ports": [
              {
                "containerPort": 8000
              }
            ],
            "resources": {
              "requests": {
                "cpu": "300m",
                "memory": "1Gi"
              },
              "limits": {
                "cpu": "750m",
                "memory": "1.5Gi"
              }
            },
            "env": [
              {
                "name": "ENVIRONMENT",
                "value": "staging"
              },
              {
                "name": "VERSION",
                "value": "3.0.0"
              },
              {
                "name": "LOG_LEVEL",
                "value": "INFO"
              }
            ],
            "livenessProbe": {
              "httpGet": {
                "path": "/health",
                "port": 8000
              },
              "initialDelaySeconds": 30,
              "periodSeconds": 10
            },
            "readinessProbe": {
              "httpGet": {
                "path": "/ready",
                "port": 8000
              },
              "initialDelaySeconds": 5,
              "periodSeconds": 5
            }
          }
        ]
      }
    }
  }
}